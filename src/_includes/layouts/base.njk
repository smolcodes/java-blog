     {% include "partials/page-header.njk" %} 
</div>
     <section class="w-full my-10 sm:w-10/12">
        {% block content %}
        {{ content | safe }}
        {% endblock %}
      </section>
 <footer class="w-full sm:w-10/12">
 {% include "partials/footer.njk" %} 
</footer>
 </main>
